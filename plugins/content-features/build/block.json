{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/content-features",
  "version": "0.1.0",
  "title": "Content Features",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "attributes": {
    "section_style": {
      "type": "string",
      "default": ""
    },
    "section_class": {
      "type": "string",
      "default": "position-relative"
    },
    "section_id": {
      "type": "string",
      "default": ""
    },
    "section_image": {
      "type": "string",
      "default": null
    },
    "section_image_class": {
      "type": "string"
    },
    "section_image_style": {
      "type": "string"
    },
    "section_block": {
      "type": "string",
      "default": ""
    },
    "container_style": {
      "type": "string",
      "default": ""
    },
    "container_class": {
      "type": "string",
      "default": "container-fluid"
    },
    "container_id": {
      "type": "string",
      "default": ""
    },
    "row_style": {
      "type": "string",
      "default": ""
    },
    "row_class": {
      "type": "string",
      "default": "row justify-content-center"
    },
    "row_id": {
      "type": "string",
      "default": ""
    },
    "features": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "default": [
        {
          "col_class": "col-lg-4 col-md-6 col-12",
          "col_style": "background:var(--accent-secondary);",
          "col_id": "",
          "col_inner_class": "d-flex align-items-center justify-content-md-center",
          "data_aos": "fade-up",
          "data_aos_delay": "",
          "title": "",
          "title_class": "bold",
          "title_style": "",
          "content": "",
          "code_block": ""
        }
      ]
    }
  },
  "textdomain": "content-features",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}